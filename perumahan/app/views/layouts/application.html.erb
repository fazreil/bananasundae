<html>
<head>
	<title>Punchware - Sistem Kutipan Bayaran Perumahan</title>
	<meta charset="utf-8" />
	<meta name="viewport" content="width=device-width, initial-scale=1" />
	<%= stylesheet_link_tag    'main.css', media: 'screen' %>
  	<%= javascript_include_tag 'main.js' %>
  	<%= javascript_include_tag 'util.js' %>
  	<%= javascript_include_tag 'skel.min.js' %>
  	<%= javascript_include_tag 'jquery.min.js' %>

</head>
<body class="subpage">
	<!-- Header -->
	<header id="header">
		<div class="inner">
			<a href="index.html" class="logo">Sistem Kutipan Bayaran Perumahan</a>
			<nav id="nav">
				<%= link_to 'Home', root_path %>
				<%= link_to 'Bayaran', bayaran_index_path %>
				<%= link_to 'Rumah', rerumah_path %>

			</nav>
			<a href="#navPanel" class="navPanelToggle"><span class="fa fa-bars"></span></a>
		</div>
	</header>
	<div class="inner">
	<%= yield %>
	</div>

	<!-- Footer -->
	<footer id="footer">
		<div class="inner">

			<h3>Get in touch</h3>

			<form action="#" method="post">

				<div class="field half first">
					<label for="name">Name</label>
					<input name="name" id="name" type="text" placeholder="Name">
				</div>
				<div class="field half">
					<label for="email">Email</label>
					<input name="email" id="email" type="email" placeholder="Email">
				</div>
				<div class="field">
					<label for="message">Message</label>
					<textarea name="message" id="message" rows="6" placeholder="Message"></textarea>
				</div>
				<ul class="actions">
					<li><input value="Send Message" class="button alt" type="submit"></li>
				</ul>
			</form>

			<div class="copyright">
				&copy; Untitled. Design: <a href="https://templated.co">TEMPLATED</a>. Images: <a href="https://unsplash.com">Unsplash</a>.
			</div>

		</div>
	</footer>

	<!-- Scripts 
	<script src="<% link_to 'Home', root_path%>assets/javascripts/jquery.min.js"></script>
	<script src="<% link_to 'Home', root_path%>assets/javascripts/skel.min.js"></script>
	<script src="<% link_to 'Home', root_path%>assets/javascripts/util.js"></script>
	<script src="<% link_to 'Home', root_path%>assets/javascripts/main.js"></script>
	-->
</body>
</html>
